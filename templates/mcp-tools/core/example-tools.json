[
  {
    "name": "filesystem_read",
    "description": "Read file contents from the filesystem",
    "parameters": [
      {
        "name": "path",
        "type": "string",
        "description": "Path to the file to read",
        "required": true
      },
      {
        "name": "encoding",
        "type": "string",
        "description": "File encoding (default: utf-8)",
        "required": false,
        "default": "utf-8"
      }
    ],
    "returns": {
      "type": "string",
      "description": "File contents"
    },
    "examples": [
      {
        "input": {
          "path": "/path/to/file.txt"
        },
        "output": "File contents here",
        "description": "Read a text file"
      }
    ]
  },
  {
    "name": "http_fetch",
    "description": "Fetch data from HTTP endpoint",
    "parameters": [
      {
        "name": "url",
        "type": "string",
        "description": "URL to fetch",
        "required": true
      },
      {
        "name": "method",
        "type": "string",
        "description": "HTTP method (GET, POST, etc.)",
        "required": false,
        "default": "GET"
      },
      {
        "name": "headers",
        "type": "object",
        "description": "HTTP headers",
        "required": false
      }
    ],
    "returns": {
      "type": "object",
      "description": "HTTP response"
    },
    "examples": [
      {
        "input": {
          "url": "https://api.example.com/data",
          "method": "GET"
        },
        "output": {
          "status": 200,
          "data": {}
        },
        "description": "Fetch data from API"
      }
    ]
  },
  {
    "name": "data_transform",
    "description": "Transform data using specified operation",
    "parameters": [
      {
        "name": "data",
        "type": "array",
        "description": "Data to transform",
        "required": true
      },
      {
        "name": "operation",
        "type": "string",
        "description": "Transform operation (map, filter, reduce)",
        "required": true
      },
      {
        "name": "expression",
        "type": "string",
        "description": "Transform expression",
        "required": true
      }
    ],
    "returns": {
      "type": "array",
      "description": "Transformed data"
    },
    "examples": [
      {
        "input": {
          "data": [1, 2, 3, 4, 5],
          "operation": "map",
          "expression": "x => x * 2"
        },
        "output": [2, 4, 6, 8, 10],
        "description": "Double each number"
      }
    ]
  }
]
